{
  "presets": [
    "@babel/env",
    "@babel/react"
  ],
  "env": {
    "development" : {
      "compact": false
    }
  },
  "plugins": [
    "styled-components",
    "@babel/plugin-transform-runtime",
    [
      "babel-plugin-direct-import",
